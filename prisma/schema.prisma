generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
}

model Transaction {
  id        Int      @id @default(autoincrement())
  type      String
  amount    Int
  note      String?
  createdAt DateTime @default(now())
}
